{
  "name": "sparkcompiler",
  "module": "index.ts",
  "type": "module",
  "version": "0.2",
  "private": true,
  "bin": {
    "spk": "./src/cli/cli.ts"
  },
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start:dev": "bun run src/index.ts",
    "start:file": "bun run src/index.ts --file",
    "start:debug": "bun run src/index.ts --ascii --debug --file",
    "start:exemple": "bun run src/index.ts --exemple --file",
    "exemple:print": "bun run start:exemple simple_print",
    "exemple:calcul": "bun run start:exemple simple_calcul",
    "exemple:if": "bun run start:exemple simple_if",
    "exemple:if_else": "bun run start:exemple if_else",
    "exemple:rev_bool": "bun run start:exemple reverse_boolean_statement",
    "exemple:while_loop": "bun run start:exemple simple_while_loop"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {}
}